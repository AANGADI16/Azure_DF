{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "jlrdemopipeline"
		},
		"AzureDataLakeStorage1ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1ls'"
		},
		"Databrickslsjlr_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Databrickslsjlr'"
		},
		"lscopyfilefromlocal_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lscopyfilefromlocal'"
		},
		"AzureDataLakeStorage1ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datasourcejlr.dfs.core.windows.net"
		},
		"lscopyfilefromlocal_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\dellpc\\Desktop\\JLR\\Raw_data"
		},
		"lscopyfilefromlocal_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Abhishek"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "jlrdemopipeline_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1ls_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1ls_accountKey')]"
					},
					"Databrickslsjlr_accessToken": {
						"value": "[parameters('Databrickslsjlr_accessToken')]"
					},
					"lscopyfilefromlocal_password": {
						"value": "[parameters('lscopyfilefromlocal_password')]"
					},
					"AzureDataLakeStorage1ls_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1ls_properties_typeProperties_url')]"
					},
					"lscopyfilefromlocal_properties_typeProperties_host": {
						"value": "[parameters('lscopyfilefromlocal_properties_typeProperties_host')]"
					},
					"lscopyfilefromlocal_properties_typeProperties_userId": {
						"value": "[parameters('lscopyfilefromlocal_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}